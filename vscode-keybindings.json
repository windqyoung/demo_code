// 将键绑定放入此文件中以覆盖默认值
[
    {
        "key": "alt+1",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+2",
        "command": "editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+3",
        "command": "editor.action.jumpToBracket",
        "when": "editorTextFocus"
    },
    {
        "key": "f2",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "f3",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.deleteLines",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+enter",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+oem_1",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+h",
        "command": "cursorLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+j",
        "command": "cursorDown",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+k",
        "command": "cursorUp",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+l",
        "command": "cursorRight",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+s",
        "command": "deleteLeft",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+d",
        "command": "deleteRight",
        "when": "editorTextFocus && !editorReadonly"
    }
]